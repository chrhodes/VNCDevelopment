<UserControl
    x:Class="VNC.WPF.Presentation.Views.UIThree_Beta"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:VNC.WPF.Presentation.Views"
    mc:Ignorable="d"
     
    d:DesignWidth="300" d:DesignHeight="300"
>
    <DockPanel Background="LightSeaGreen" Margin="10">
        <TextBlock DockPanel.Dock="Top" HorizontalAlignment="Center" FontSize="20" Text="UIThree Beta"  />
        <TextBlock DockPanel.Dock="Bottom" HorizontalAlignment="Center" Text="{Binding Message}" />
        <Button Click="Button_Click" Margin="20" FontSize="20">Zoom Three</Button>
    </DockPanel>
    
</UserControl>
